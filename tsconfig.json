{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "declaration": false,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
  }
}